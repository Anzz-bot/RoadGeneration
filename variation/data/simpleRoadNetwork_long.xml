<roadNetwork xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../xml/input.xsd">
    <vars>
        <var id="halfhalf8" type="lindep" dp="(halfhalf7/3)*4*1666*7" />
        <var id="halfhalf7" type="lindep" dp="(halfhalf6/3)*4*6*7" />
        <var id="halfhalf6" type="lindep" dp="(halfhalf5/3)*4*6*7" />
        <var id="halfhalf5" type="lindep" dp="(halfhalf4/3)*4*6*7" />
        <var id="halfhalf4" type="lindep" dp="(halfhalf3/3)*4*6*7" />
        <var id="halfhalf3" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf9" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3a" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3b" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3c" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3d" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3e" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3f" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3r" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3g" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3h" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3i" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf3j" type="lindep" dp="(halfhalf2/3)*4*6*7" />
        <var id="halfhalf2" type="lindep" dp="halfhalf/3" />
        <var id="halfhalf" type="lindep" dp="halfANDrad/2" /> 
        <var id="halfANDrad" type="lindep" dp="angleT*0.3 + radiusCR" />        
        <var id="radiusCR" type="normal" mu="150" sd="11.5" />        
        <var id="angleT" type="normal" mu="1.57" sd="0.12" />
             
    </vars>
    <segments>

        <junction id="1" type="MA">
            <road id="1" classification="main" >	
                <referenceLine>
                    <spiral length="50"  Rs="0" Re="-300"/>
                    <arc length="100" R="-300"/>
                    <spiral length="90"  Rs="-300" Re="0"/>
                </referenceLine>
                <lanes>
                </lanes>
            </road>
            <road id="2" classification="access" >	
                <referenceLine>
                    <line length="${halfhalf}"/>
                </referenceLine>
            </road>
            <intersectionPoint refRoad="1" s="100">
                <adRoad id="2" s="0" angle="${angleT}"/>
            </intersectionPoint>
        </junction>

        <junction id="2" type="2M">
            <road id="1" classification="main" >	
                <referenceLine>
                    <line length="200"/>
                </referenceLine>
            </road>
            <road id="2" classification="access" >	
                <referenceLine>
                    <line length="200" />
                </referenceLine>
            </road>
            <intersectionPoint refRoad="1" s="100">
                <adRoad id="2" s="100" angle="1.57"/>
            </intersectionPoint>
        </junction>
        <connectingRoad id="3">
            <road id="1" classification="access" >	
                <referenceLine>
                    <line length="20"/>                    
                    <spiral length="50"  Rs="0" Re="${radiusCR}"/>
                    <arc length="50" R="${radiusCR}"/>
                    <spiral length="50"  Rs="${radiusCR}" Re="0"/>
                </referenceLine>
            </road>
        </connectingRoad>        
    </segments>
    <links refId="2" hdgOffset="0.0" xOffset="0" yOffset="0">
        <segmentLink fromSegment="2" toSegment="3" fromRoad="1" toRoad="1" fromPos="end" toPos="start" />
        <segmentLink fromSegment="3" toSegment="1" fromRoad="1" toRoad="1" fromPos="end" toPos="end" />
    </links>
</roadNetwork>
